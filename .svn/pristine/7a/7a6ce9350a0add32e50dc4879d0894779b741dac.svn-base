<script id="scriptDiv1" type="text/html">
	<ul>
    <% for (var i = 0; i < list.length; i ++) { %>
    	<li>
			<div>
				<div class="td-list-top">
					<p class="td-list-top-l"><%=list[i].log%></p>
					<p class="td-list-top-r"><%=list[i].money%> 元</p>
				</div>
				<div class="td-list-bottom">
					<p class="td-list-bottom-l">交易成功</p>
					
					<p class="td-list-bottom-c" id="time"><%=list[i].ctime%></p>
				</div>
			</div>
		</li>
    <% } %>
    </ul>
</script>
<script>
	var uid;
	var allResult = [];
	var userInfo = null;
	// var url = 0defaultRandomEarnScoreUrl;
	var messageList = null;
	var openHtml = null;

	apiready = function () {
		var getMoneyJson = {};
		new datalistModel(addDataToHtml,getMyWalletPagerUrl,getMoneyJson,10,5,100,true);
	};
	function addDataToHtml(results, append) {
		var message = {};
		for(var i=0;i<results.length;i++){
			allResult[results[i].id]=results[i];
			results[i].ctime = formatTimeToDate(results[i].ctime);
		}
		var data = {list: results};
		var html = template('scriptDiv1', data);
		if (append) {
			var div = document.createElement("div");
			div.innerHTML = html;
			document.getElementById('mainOne').appendChild(div);
		} else {
			document.getElementById('mainOne').innerHTML = html;
		} 
	}

</script>
