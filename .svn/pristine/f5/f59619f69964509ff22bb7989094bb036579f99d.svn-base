        .list li:first-child{font-size: 16px;height: 20px;line-height: 20px;}
        .list li:last-child{height: 40px;font-size: 14px;line-height: 20px;overflow: hidden;
            text-overflow: ellipsis;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}
        .list li span{display: block;float: left;}
        .list li:last-child{color: #9296a3;}


